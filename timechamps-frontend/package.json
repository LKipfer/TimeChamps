{
  "name": "timechamps-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint:js": "eslint src --ext .ts,.vue",
    "format:test": "prettier 'src/**/*.{ts,js,html,css,scss,less,md}' --list-different",
    "format:write": "npm run lint:js -- --fix && prettier 'src/**/*.{ts,js,html,css,scss,less,md}' --write"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "axios": "^1.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primevue": "^3.21.0",
    "vue": "^3.2.45",
    "vue-eslint-parser": "^9.1.0",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.29.0",
    "eslint-plugin-vue": "^9.8.0",
    "prettier": "2.8.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.1",
    "vue-tsc": "^1.0.13"
  }
}
